{
  "tasks": {
    "dev": "deno run -P --env-file --watch main.ts",
    "start": "deno run -P main.ts"
  },
  "imports": {
    "@astral/astral": "jsr:@astral/astral@^0.5.3",
    "@b-fuze/deno-dom": "jsr:@b-fuze/deno-dom@^0.1.56",
    "@std/assert": "jsr:@std/assert@1",
    "@types/nodemailer": "npm:@types/nodemailer@^7.0.1",
    "nodemailer": "npm:nodemailer@^7.0.6"
  },
  "permissions": {
    "default": {
      "env": true,
      "net": true,
      "run": true,
      "read": true,
      "write": true,
      "sys": true
    }
  }
}
